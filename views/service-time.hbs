<!-- timesheet section start -->
  <section id="timesheet">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <h3>EMPLOYEE DAILY TIME SHEET</h3>
          <hr>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-3 col-xs-12">
          <label for="empid">Employee ID<sup>*</sup>:</label>
        </div>
        <div class="col-lg-9 col-xs-12">
          <input type="text" id="empid" name="empid" required><br>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-3 col-xs-12">
          <label for="emp-name">Employee Name<sup>*</sup>:</label>
        </div>
        <div class="col-lg-9 col-xs-12">
          <input type="text" id="emp-name" name="emp-name" required><br>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-3 col-xs-12">
          <label for="working-day">Select Day<sup>*</sup>:</label>
        </div>
        <div class="col-lg-9 col-xs-12">
          <input type="date" id="working-day" name="working-day" required><br>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-3 col-xs-12">
          <label for="efforts">Efforts<sup>*</sup>:</label>
        </div>
        <div class="col-lg-9 col-xs-12">
          <input type="text" id="efforts" name="efforts" placeholder="hh:mm" required><br>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-3 col-xs-12">
          <label for="comments">Additional Comments(optional)</label>
        </div>
        <div class="col-lg-9 col-xs-12">
          <textarea id="timeentry-comment" rows="4" cols="50" placeholder="Add Comment"></textarea>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-3 col-xs-12">
        </div>
        <div class="col-lg-9 col-xs-12">
          <br>
          <button class="btn btn-primary" type="submit" id="time-entry">Submit</button>
        </div>
      </div>
    </div>
    </div>
  </section>
  <!-- timesheet section end -->